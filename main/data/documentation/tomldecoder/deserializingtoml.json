"identifier":"doc://TOMLDecoder/documentation/TOMLDecoder/TOMLError","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"TOMLError"}],"role":"symbol","title":"TOMLError","type":"topic","url":"/documentation/tomldecoder/tomlerror"},"doc://TOMLDecoder/documentation/TOMLDecoder/TOMLTable":{"abstract":[{"text":"A parsed TOML table. Entry point for parsing TOML data.","type":"text"},{"text":" ","type":"text"},{"text":"The root of a TOML document is always a table.","type":"text"}],"fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"TOMLTable"}],"identifier":"doc://TOMLDecoder/documentation/TOMLDecoder/TOMLTable","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"TOMLTable"}],"role":"symbol","title":"TOMLTable","type":"topic","url":"/documentation/tomldecoder/tomltable"},"doc://TOMLDecoder/documentation/TOMLDecoder/TOMLTable/init(source:)-1kv4z":{"abstract":[{"text":"Create a root-level TOML table from a TOML document.","type":"text"}],"fragments":[{"kind":"identifier","text":"init"},{"kind":"text","text":"("},{"kind":"externalParam","text":"source"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:SS","text":"String"},{"kind":"text","text":") "},{"kind":"keyword","text":"throws"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"s:11TOMLDecoder9TOMLErrorV","text":"TOMLError"},{"kind":"text","text":")"}],"identifier":"doc://TOMLDecoder/documentation/TOMLDecoder/TOMLTable/init(source:)-1kv4z","kind":"symbol","role":"symbol","title":"init(source:)","type":"topic","url":"/documentation/tomldecoder/tomltable/init(source:)-1kv4z"},"https://toml.io":{"identifier":"https://toml.io","title":"toml.io","titleInlineContent":[{"text":"toml.io","type":"text"}],"type":"link","url":"https://toml.io"}},"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"seeAlsoSections":[{"anchor":"Deserializing-TOML","generated":true,"identifiers":["doc://TOMLDecoder/do