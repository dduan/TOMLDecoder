{"abstract":[{"text":"A Offset Date-Time as defined by TOML.","type":"text"}],"hierarchy":{"paths":[["doc://TOMLDecoder/documentation/TOMLDecoder"]]},"identifier":{"interfaceLanguage":"swift","url":"doc://TOMLDecoder/documentation/TOMLDecoder/OffsetDateTime"},"kind":"symbol","metadata":{"externalID":"s:11TOMLDecoder14OffsetDateTimeV","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"OffsetDateTime"}],"modules":[{"name":"TOMLDecoder"}],"navigatorTitle":[{"kind":"identifier","text":"OffsetDateTime"}],"role":"symbol","roleHeading":"Structure","symbolKind":"struct","title":"OffsetDateTime"},"primaryContentSections":[{"declarations":[{"languages":["swift"],"platforms":["macOS"],"tokens":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"OffsetDateTime"}]}],"kind":"declarations"},{"content":[{"anchor":"overview","level":2,"text":"Overview","type":"heading"},{"inlineContent":[{"text":"A offset date-time represents a instant in time.","type":"text"},{"text":" ","type":"text"},{"text":"Conceptually ","type":"text"},{"identifier":"doc://TOMLDecoder/documentation/TOMLDecoder/OffsetDateTime","isActive":true,"type":"reference"},{"text":" simialar to ","type":"text"},{"code":"Foundation.Date","type":"codeVoice"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"Operationally, ","type":"text"},{"identifier":"doc://TOMLDecoder/documentation/TOMLDecoder/OffsetDateTime","isActive":true,"type":"reference"},{"text":" works more like ","type":"text"},{"code":"Foundation.DateComponents","type":"codeVoice"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"It stores the components, except each component is non-optional.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"identifier":"doc://TOMLDecoder/documentation/TOMLDecoder/OffsetDateTime","isActive":true,"type":"reference"},{"text":" stores fractional seconds to the nanosecond precision.","type":"text"},{"text"